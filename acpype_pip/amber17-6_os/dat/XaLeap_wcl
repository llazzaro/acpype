!
!	Dialog boxes
!
!
! ---- edit <unit|parmset>
!
!
! -- form definition
!
XaLeap*dbEdit.wcClassName:			TransientShell
XaLeap*dbEdit.wcChildren:			dForm
XaLeap*dbEdit.wcCallback:			SetParentShellAsGroup(/)
XaLeap*dbEdit.title:			XLEaP: Edit Unit/Parmset
XaLeap*dbEdit.popdownCallback:		WcSetSensitiveCB(*editSme) xtcpXACDestroyList(/*editUnitList)
XaLeap*dbEdit.popupCallback:		xtcpXACReplaceShell() \
					WcSetInsensitiveCB(*editSme) xtcpXACInitializeList(/*editUnitList,UP)  \
	WcSetValueCB((this.height:400) (this.width:500))

XaLeap*dbEdit.dForm.wcClassName:		Paned
XaLeap*dbEdit.dForm.wcChildren:		editUnitForm sprt commFrame
XaLeap*dbEdit.dForm.orientation:		vertical
XaLeap*dbEdit.dForm.vSpace:			4
XaLeap*dbEdit.dForm.hSpace:			4
XaLeap*dbEdit.dForm.shadowWidth:		2
XaLeap*dbEdit.dForm.internalBorderWidth:	0

XaLeap*dbEdit*sprt:wcClassName:		Separator


XaLeap*dbEdit.dForm.editUnitForm.showGrip:		False
XaLeap*dbEdit.dForm.commFrame.showGrip:	False
XaLeap*dbEdit.dForm.commFrame.skipAdjust:	True

XaLeap*dbEdit.dForm.editUnitForm.left:		ChainLeft
XaLeap*dbEdit.dForm.editUnitForm.right:		ChainForm
XaLeap*dbEdit.dForm.editUnitForm.top:			ChainTop
XaLeap*dbEdit.dForm.editUnitForm.bottom:		ChainBottom

XaLeap*dbEdit.dForm.sprt.wcClassName:	Separator
XaLeap*dbEdit.dForm.sprt.left:		ChainLeft
XaLeap*dbEdit.dForm.sprt.right:		ChainForm
XaLeap*dbEdit.dForm.sprt.top:		ChainTop
XaLeap*dbEdit.dForm.sprt.bottom:		ChainBottom
XaLeap*dbEdit.dForm.sprt.fromVert:		^editUnitForm

XaLeap*dbEdit.dForm.commFrame.left:		ChainLeft
XaLeap*dbEdit.dForm.commFrame.right:	ChainForm
XaLeap*dbEdit.dForm.commFrame.top:		ChainBottom
XaLeap*dbEdit.dForm.commFrame.bottom:	ChainForm
XaLeap*dbEdit.dForm.commFrame.fromVert:	^sprt


XaLeap*dbEdit.dForm.commFrame.wcClassName:	Frame
XaLeap*dbEdit.dForm.commFrame.wcChildren:	dAccept  dCancel
XaLeap*dbEdit.dForm.commFrame.layoutPolicy:	fraction
XaLeap*dbEdit.dForm.commFrame.shadowWidth:	0
!! 
!! 	DO NOT Popdown the dialog box before you parse the command!! 
!! 		Otherwise values could be read from empty lists.
!! 
