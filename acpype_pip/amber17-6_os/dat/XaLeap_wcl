XaLeap*loadFileSelector.internalBorderWidth:			0
XaLeap*loadFileSelector*showGrip:				False
XaLeap*loadFileSelector.orientation:				vertical
XaLeap*loadFileSelector.shadowWidth:				0
XaLeap*loadFileSelector.wcChildren:				fileListVp 	\
						blnk		\
						listViewPort 	\
						blnk		\
						fileName

XaLeap*loadFileSelector*blnk.wcClassName:			Simple
